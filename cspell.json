{
	"language": "en-GB",
	"ignorePaths": [
		"**/node_modules/**",
		"LICENSE",
		"**/coverage/**",
		"**/dist/**",
		"**/docs/reference/**",
		"**/docs/open-api/**",
		"**/tests/**",
		"*.yaml",
		"package.json",
		"rollup.config.mjs",
		"vitest.config.*",
		"tsconfig.json",
		"typedoc.json",
		".eslintrc.cjs",
		"**/wordlists/**",
		"**/src-json/**",
		"**/ldContexts/*.json",
		"changelog.md"
	],
	"words": [
		"abbrev",
		"bday",
		"cefact",
		"cryptosuite",
		"DCAT",
		"eddsa",
		"EORI",
		"epc",
		"epcis",
		"EUID",
		"GLEIF",
		"gteq",
		"iepc",
		"jaif",
		"lgtin",
		"lteq",
		"masterdata",
		"Multibase",
		"Multikey",
		"nameof",
		"Odrl",
		"rdfc",
		"sgln",
		"sgtin",
		"Stiftung",
		"unece",
		"vcard",
		"xone",
		"DCMI",
		"dcmitype",
		"dublincore"
	],
	"ignoreRegExpList": [
		"/from\\s+(['\"]).*\\1/",
		"/eslint-disable.*/",
		"/^```shell[\\s\\S]*?```$/gm",
		"/^```json[\\s\\S]*?```$/gm"
	]
}
